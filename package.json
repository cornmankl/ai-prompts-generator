{
  "name": "ai-prompts-generator",
  "version": "1.0.0",
  "description": "Advanced AI Prompts Generator with Context Engineering and 2025 Features",
  "main": "server/index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build": "npm run client:build && npm run server:build",
    "start": "concurrently \"npm run server:start\" \"npm run client:preview\"",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "client:lint": "cd client && npm run lint",
    "server:dev": "nodemon server/index.js",
    "server:build": "echo \"Server build not needed for Node.js\"",
    "server:start": "node server/index.js",
    "server:lint": "echo \"Server linting not configured\"",
    "lint": "npm run client:lint && npm run server:lint",
    "test": "echo \"Tests not implemented yet\"",
    "install-all": "npm install && cd client && npm install",
    "clean": "rm -rf client/dist client/node_modules server/node_modules node_modules",
    "live": "live-server --port=3000 --open=/index.html",
    "preview": "npm run client:build && npm run live",
    "serve": "node server.js",
    "preview-full": "npm run client:build && npm run serve"
  },
  "keywords": [
    "ai",
    "prompts",
    "generator",
    "context-engineering",
    "llm"
  ],
  "author": "AI Prompts Generator Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.6.0",
    "marked": "^9.1.6",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "prismjs": "^1.29.0",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.2"
  }
}
