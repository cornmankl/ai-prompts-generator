{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/public",
  "installCommand": "cd client && npm install",
  "devCommand": "npm run dev",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": [
    "iad1"
  ]
}